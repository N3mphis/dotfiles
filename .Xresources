Xcursor.theme: /usr/share/icons/Breeze-Amber-Cursor

emacs.font: DejaVu Sans Mono-12

! clean up urxvt, add keybinds
URxvt.intensityStyles: false
URxvt.scrollBar: false
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007
URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger: C-Up
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.geometry: 105x40

! icon file
URxvt*iconFile: /usr/share/icons/Paper/scalable/apps/utilities-terminal-symbolic.svg
! font
!URxvt*font: xft:Source\ Code\ Pro\ for\ Powerline:size=12:antialias=true:hinting=true
!URxvt*font: xft:Fira\ Mono\ for\ Powerline:size=12:antialias=true:hinting=true
URxvt*font: xft:Droid\ Sans\ Mono\ for\ Powerline:size=13:antialias=true:hinting=true

! terminal padding
URxvt.internalBorder: 20

! uncomment for transparent terminal if you are ready to be made fun of in #rice
! URxvt.transparent:	true
! URxvt.shading:		15

! make otf / ttf fonts look pretty.  does not matter if you are using a bitmap font.
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb
Xft.dpi: 96


! clickable links, clipboard support, mouse wheel support, and on the fly font resizing
! you will need to install the relevent perl extensions for these to work
URxvt.perl-ext-common: default,matcher,clipboard,vtwheel,resize-font,fullscreen
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1
URxvt.matcher.rend.0: Uline Bold fg5
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.keysym.F11: perl:fullscreen:switch

! do not scroll with output
URxvt*scrollTtyOutput: false

! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true

! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true

